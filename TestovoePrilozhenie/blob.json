    "documentalgorithms": [
      {
        "script": {
          "title": "script",
          "sources": "//Проведение документа\r\nfunction activate(currentDocument) {\r\n     if (!notValid(currentDocument)) {\r\n         for (let tableRec of currentDocument.getRows()){\r\n            //создание и заполнение проводки\r\n            let posting \u003d EntityFactory.createPosting();\r\n            posting.setComment(tableRec.get(\u0027TestovoePrilozhenie.Dokument.tr.Naimenovanie\u0027));\r\n            posting.setValue(tableRec.get(\u0027TestovoePrilozhenie.Dokument.tr.Summa\u0027));\r\n            posting.setDebit(tableRec.get(\u0027TestovoePrilozhenie.Dokument.tr.Debet\u0027));\r\n            posting.setCredit(tableRec.get(\u0027TestovoePrilozhenie.Dokument.tr.Kredit\u0027));\r\n            currentDocument.addPosting(posting);\r\n        };\r\n        //устанавливаем текущую дату проводки\r\n        currentDocument.setExecuted();\r\n\r\n\r\n        //сохранение документа с проводками\r\n        currentDocument.save();\r\n        \r\n        /*let catalog \u003d EntityFactory.createCatalogRecord(\u0027TestovoePrilozhenie.Klassifikator\u0027);\r\n        catalog.setCode(\u00270001\u0027);\r\n        catalog.setTitle(\u0027Первый\u0027);\r\n        catalog.set(\u0027TestovoePrilozhenie.Klassifikator.Summa\u0027, \u0027100\u0027);\r\n        return catalog;\r\n        catalog.save();*/\r\n        \r\n        let catalog \u003d EntityFactory.createCatalogRecord(\u0027TestovoePrilozhenie.RoditelskiiiSpravochnik\u0027);\r\n        catalog.setCode(\u002700000000000000000001\u0027);\r\n        catalog.setTitle(\u0027Добавлена новая запись в справочник\u0027);\r\n        catalog.set(\u0027TestovoePrilozhenie.RoditelskiiiSparvochnik.PolnoeNaimenovanie\u0027, \u0027Описание\u0027);\r\n        catalog.set(\u0027TestovoePrilozhenie.RoditelskiiiSpravochnik.KratkoeNaimenovanie\u0027, \u0027Описание краткое\u0027);\r\n        return catalog;\r\n        catalog.save();\r\n        \r\n        //addRecordOnSpravochnik();\r\n        return;  \r\n    } else \r\n        return notValid(currentDocument);\r\n}\r\n\r\n//Снятие документа с проведения\r\nfunction deactivate(currentDocument) {\r\n    currentDocument.unPostingAndSave();\r\n}\r\n\r\n//Проверка документа на полное заполнение\r\nfunction notValid(currentDocument) { \r\n    let resultValidate \u003d currentDocument.validate();\r\n \r\n    /*if (!currentDocument.getRows().size()){\r\n        resultValidate.addValueError(\u0027Не заполнена табличная часть документа.\u0027);\r\n    }*/\r\n\r\n    if (resultValidate.hasErrors() || resultValidate.hasValueErrors() ) \r\n        return  resultValidate;\r\n}"
        },
